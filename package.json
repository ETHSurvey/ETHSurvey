{
  "name": "ethsurvey",
  "version": "0.0.1",
  "homepage": ".",
  "scripts": {
    "start": "webpack-dev-server --mode=development",
    "build": "webpack --mode=production",
    "lint": "tslint --project './tsconfig.json'",
    "lint:fix": "tslint --project './tsconfig.json' --fix \"./src/**/*.{ts,tsx}\"",
    "prettier-ts": "prettier --single-quote --parser typescript --write \"./src/**/*.{ts,tsx}\"",
    "prettier-less": "prettier --single-quote --parser less --write \"./src/**/*.less\"",
    "typechain": "typechain --target=web3-1.0.0 --outDir ./src/types './build/contracts/*.json'"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "npm run prettier-ts",
      "npm run lint:fix",
      "git add"
    ],
    "./src/**/*.less": [
      "npm run prettier-less",
      "git add"
    ]
  },
  "dependencies": {
    "antd": "^3.22.0",
    "connected-react-router": "^6.5.2",
    "history": "^4.9.0",
    "identicon.js": "^2.3.3",
    "immutable": "^4.0.0-rc.12",
    "ipfs-http-client": "^33.1.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-observable": "^1.1.0",
    "rxjs": "^6.5.2",
    "shortid": "^2.2.14",
    "truffle-environment": "^0.1.8",
    "truffle-hdwallet-provider": "1.0.16",
    "underscore.string": "^3.3.5",
    "web3": "^1.2.1"
  },
  "devDependencies": {
    "@types/history": "^4.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-router-redux": "^5.0.18",
    "@types/redux-actions": "^2.6.1",
    "@types/redux-logger": "^3.0.7",
    "@types/shortid": "0.0.29",
    "@types/web3": "^1.0.19",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.4",
    "json-loader": "^0.5.7",
    "less": "^3.10.1",
    "less-loader": "^5.0.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^9.2.3",
    "openzeppelin-solidity": "^2.3.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "redux-logger": "^3.0.6",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "truffle-legacy-system": "^1.0.7",
    "truffle-solidity-loader": "0.1.31",
    "ts-import-plugin": "^1.6.1",
    "ts-loader": "^6.0.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.6.0",
    "typechain": "^0.3.19",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
