{
  "name": "ETHSurvey",
  "version": "0.0.1",
  "homepage": ".",
  "scripts": {
    "start": "webpack-dev-server --mode=development",
    "build": "rm -rf build/ && webpack --mode=production",
    "lint": "tslint --project './tsconfig.json'",
    "lint:fix": "tslint --project './tsconfig.json' --fix \"./src/**/*.{ts,tsx}\"",
    "precommit": "lint-staged && npm run build",
    "prettier-ts": "prettier --single-quote --parser typescript --write \"./src/**/*.{ts,tsx}\"",
    "prettier-less": "prettier --single-quote --parser less --write \"./src/**/*.less\"",
    "postinstall": "rm -f node_modules/web3/index.d.ts"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "npm run prettier-ts",
      "npm run lint:fix",
      "git add"
    ],
    "./src/**/*.less": [
      "npm run prettier-less",
      "git add"
    ]
  },
  "dependencies": {
    "antd": "^3.8.2",
    "history": "^4.7.2",
    "identicon.js": "^2.3.2",
    "immutable": "^3.8.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.2.2",
    "truffle-contract": "^3.0.6",
    "underscore.string": "^3.3.4",
    "web3": "^1.0.0-beta.35"
  },
  "devDependencies": {
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.6",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-router-redux": "^5.0.15",
    "@types/redux-actions": "^2.3.0",
    "@types/redux-logger": "^3.0.6",
    "@types/web3": "^1.0.3",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "json-loader": "^0.5.7",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^7.2.2",
    "openzeppelin-solidity": "^1.12.0",
    "prettier": "^1.14.2",
    "prop-types": "^15.6.2",
    "redux-logger": "^3.0.6",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.22.1",
    "truffle-solidity-loader": "^0.1.0",
    "ts-import-plugin": "^1.5.4",
    "ts-loader": "^4.5.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
