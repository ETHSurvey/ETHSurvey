{
  "name": "ETHSurvey",
  "version": "0.0.1",
  "homepage": ".",
  "scripts": {
    "start": "webpack-dev-server --mode=development",
    "build": "rm -rf build/ && webpack --mode=production",
    "lint": "tslint --project './tsconfig.json'",
    "lint:fix": "tslint --project './tsconfig.json' --fix \"./src/**/*.{ts,tsx}\"",
    "prettier-ts": "prettier --single-quote --parser typescript --write \"./src/**/*.{ts,tsx}\"",
    "prettier-less": "prettier --single-quote --parser less --write \"./src/**/*.less\"",
    "postinstall": "rm -f node_modules/web3/index.d.ts",
    "typechain": "typechain --target=web3-1.0.0 --outDir ./src/types './build/contracts/*.json'"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "npm run prettier-ts",
      "npm run lint:fix",
      "git add"
    ],
    "./src/**/*.less": [
      "npm run prettier-less",
      "git add"
    ]
  },
  "dependencies": {
    "antd": "^3.10.8",
    "history": "^4.7.2",
    "identicon.js": "^2.3.3",
    "immutable": "^4.0.0-rc.12",
    "ipfs-api": "^26.1.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.3.3",
    "shortid": "^2.2.14",
    "underscore.string": "^3.3.5",
    "web3": "^1.0.0-beta.36"
  },
  "devDependencies": {
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.10",
    "@types/react-redux": "^6.0.10",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-router-redux": "^5.0.16",
    "@types/redux-actions": "^2.3.1",
    "@types/redux-logger": "^3.0.6",
    "@types/shortid": "0.0.29",
    "@types/web3": "^1.0.12",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.0",
    "json-loader": "^0.5.7",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^8.1.0",
    "openzeppelin-solidity": "^2.0.0",
    "prettier": "^1.15.2",
    "prop-types": "^15.6.2",
    "redux-logger": "^3.0.6",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "truffle-solidity-loader": "0.1.0",
    "ts-import-plugin": "^1.5.5",
    "ts-loader": "^5.3.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-loader": "^3.6.0",
    "typechain": "^0.3.9",
    "typescript": "^3.1.6",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
